  "*.{js,jsx,ts,tsx}": ["eslint", "prettier --write"],
  "*.vue": ["prettier --write"],
  "package.json": ["prettier --write"],
  "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": ["prettier --parser json --write"],
